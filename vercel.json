{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "LLM_PROVIDER": "@llm_provider",
    "OPENAI_API_KEY": "@openai_api_key",
